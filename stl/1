#include <iostream>
#include <string>
#include <hash_map>

int main()
{
  std::hash_map<int ,std::string> ismap;
  ismap[1]="11111";
  ismap[2]="22222";
  ismap[3]="33333";
	
  std::cout<<ismap[3]<<std:endl;
  std::cout<<ismap[2]<<std:endl;
  std::cout<<ismap[1]<<std:endl;
  
  return 0;
}
